<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.FileResourceBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceBean">
		<map-to xml="fileResource"/>
		<field name="fileType" type="string">
			<bind-xml name="fileType" node="element"/>
		</field>
		<field name="dataFile" type="string">
			<bind-xml name="dataFile" node="attribute"/>
		</field>
		<field name="referenceUri" type="string">
			<bind-xml name="referenceUri" node="element"/>
		</field>
	</class>
</mapping>
