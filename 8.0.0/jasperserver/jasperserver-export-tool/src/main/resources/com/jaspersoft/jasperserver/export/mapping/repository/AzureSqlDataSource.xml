<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
    <class name="com.jaspersoft.jasperserver.export.modules.repository.beans.AzureSqlDataSourceBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.JdbcDataSourceBean">
        <map-to xml="azureSqlDataSource"/>
        <field name="subscriptionId" type="string">
            <bind-xml name="subscriptionId" node="element"/>
        </field>
        <field name="keyStoreResource" type="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceReferenceBean">
            <bind-xml name="keyStoreResource" node="element"/>
        </field>
        <field name="keyStorePassword" type="string">
            <bind-xml name="keyStorePassword" node="element"/>
        </field>
        <field name="keyStoreType" type="string">
            <bind-xml name="keyStoreType" node="element"/>
        </field>
        <field name="serverName" type="string">
            <bind-xml name="serverName" node="element"/>
        </field>
        <field name="dbName" type="string">
            <bind-xml name="dbName" node="element"/>
        </field>
    </class>
</mapping>