<module xmlns="urn:jboss:module:1.1" name="jaspersoft.org.apache.batik">


    <dependencies>
        <!--
              2013-04-19  thorick

                          note that javax.api does not include
                             org.w3c.dom.svg
                          for that we pull in xml-apis-ext-1.3.04.jar
        -->
        <module name="javax.api"/>
        <system export="true">
            <paths>
                <path name="org/w3c/dom/css"/>
                <path name="org/w3c/dom/xpath"/>
                <path name="org/w3c/dom/stylesheets"/>
            </paths>
        </system>
    </dependencies>


    <resources>
        <resource-root path="batik-anim-1.14.jar"/>
        <resource-root path="batik-awt-util-1.14.jar"/>
        <resource-root path="batik-bridge-1.14.jar"/>
        <resource-root path="batik-constants-1.14.jar"/>
        <resource-root path="batik-css-1.14.jar"/>
        <resource-root path="batik-dom-1.14.jar"/>
        <resource-root path="batik-ext-1.14.jar"/>
        <resource-root path="batik-gvt-1.14.jar"/>
        <resource-root path="batik-i18n-1.14.jar"/>
        <resource-root path="batik-parser-1.14.jar"/>
        <resource-root path="batik-script-1.14.jar"/>
        <resource-root path="batik-svg-dom-1.14.jar"/>
        <resource-root path="batik-svggen-1.14.jar"/>
        <resource-root path="batik-transcoder-1.14.jar"/>
        <resource-root path="batik-util-1.14.jar"/>
        <resource-root path="batik-xml-1.14.jar"/>
        <resource-root path="batik-shared-resources-1.14.jar"/>
        <resource-root path="xml-apis-ext-1.3.04.jar"/>
        <resource-root path="xmlgraphics-commons-2.6.jar"/>
    </resources>
</module>

 
