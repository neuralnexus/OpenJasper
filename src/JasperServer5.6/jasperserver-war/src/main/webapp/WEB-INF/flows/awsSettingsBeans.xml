<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <!-- Backing action for edit AWS Settings flow -->
    <bean id="awsSettingsAction" class="com.jaspersoft.jasperserver.war.action.AwsSettingsAction">
        <property name="auditContext" ref="${bean.auditContext}"/>
        <property name="propertiesManagementService" ref="propertiesManagementService"/>
    </bean>

</beans>