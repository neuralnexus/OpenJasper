<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.auth.beans.RoleBean">
		<map-to xml="role"/>
		<field name="roleName" type="string">
			<bind-xml name="name" node="element"/>
		</field>
		<field name="tenantId" type="string">
			<bind-xml name="tenantId" node="element"/>
		</field>
		<field name="externallyDefined" type="boolean">
			<bind-xml name="externallyDefined" node="element"/>
		</field>
	</class>
</mapping>
