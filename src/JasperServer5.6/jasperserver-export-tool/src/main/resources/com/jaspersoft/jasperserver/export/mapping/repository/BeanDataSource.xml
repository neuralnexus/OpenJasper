<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.BeanDataSourceBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceBean">
		<map-to xml="beanDataSource"/>
		<field name="beanName" type="string">
			<bind-xml name="beanName" node="element"/>
		</field>
		<field name="beanMethod" type="string">
			<bind-xml name="beanMethod" node="element"/>
		</field>
	</class>
</mapping>
