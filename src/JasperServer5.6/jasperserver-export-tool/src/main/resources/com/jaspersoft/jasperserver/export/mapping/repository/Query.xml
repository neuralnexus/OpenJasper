<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.QueryBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceBean">
		<map-to xml="query"/>
		<field name="language" type="string">
			<bind-xml name="language" node="element"/>
		</field>
		<field name="queryString" type="string">
			<bind-xml name="queryString" node="element"/>
		</field>
		<field name="dataSource" type="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceReferenceBean">
			<bind-xml name="dataSource" node="element"/>
		</field>
	</class>
</mapping>
