<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.ContentResourceBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceBean">
		<map-to xml="contentResource"/>
		<field name="fileType" type="string">
			<bind-xml name="fileType" node="element"/>
		</field>
		<field name="dataFile" type="string">
			<bind-xml name="dataFile" node="attribute"/>
		</field>
		<field name="resources" type="com.jaspersoft.jasperserver.export.modules.repository.beans.ContentResourceBean" collection="array">
			<bind-xml name="subResource" node="element"/>
		</field>
	</class>
</mapping>
