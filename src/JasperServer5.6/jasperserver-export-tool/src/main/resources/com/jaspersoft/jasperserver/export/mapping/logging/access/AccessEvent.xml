<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.logging.access.beans.AccessEventBean">
		<map-to xml="accessEvent"/>
        <field name="userName" type="string">
            <bind-xml name="userName" node="element"/>
        </field>
        <field name="tenantId" type="string">
            <bind-xml name="tenantId" node="element"/>
        </field>
        <field name="eventDate" type="date">
            <bind-xml name="eventDate" node="element"/>
        </field>
        <field name="updating" type="boolean">
            <bind-xml name="updating" node="element"/>
        </field>
        <field name="resourceUri" type="string">
            <bind-xml name="resourceUri" node="element"/>
        </field>
	</class>
</mapping>
