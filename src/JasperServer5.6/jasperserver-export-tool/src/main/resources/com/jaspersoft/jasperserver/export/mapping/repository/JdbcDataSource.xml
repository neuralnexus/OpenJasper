<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.JdbcDataSourceBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceBean">
		<map-to xml="jdbcDataSource"/>
		<field name="driverClass" type="string">
			<bind-xml name="driver" node="element"/>
		</field>
		<field name="connectionUrl" type="string">
			<bind-xml name="connectionUrl" node="element"/>
		</field>
		<field name="connectionUsername" type="string">
			<bind-xml name="connectionUser" node="element"/>
		</field>
		<field name="connectionPassword" type="string">
			<bind-xml name="connectionPassword" node="element"/>
		</field>
		<field name="timezone" type="string">
			<bind-xml name="timezone" node="element"/>
		</field>
	</class>
</mapping>
