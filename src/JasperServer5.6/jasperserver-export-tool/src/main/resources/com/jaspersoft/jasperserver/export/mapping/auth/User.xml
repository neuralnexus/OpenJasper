<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.auth.beans.UserBean">
		<map-to xml="user"/>
		<field name="username" type="string">
			<bind-xml name="username" node="element"/>
		</field>
		<field name="fullName" type="string">
			<bind-xml name="fullName" node="element"/>
		</field>
		<field name="password" type="string">
			<bind-xml name="password" node="element"/>
		</field>
		<field name="emailAddress" type="string">
			<bind-xml name="email" node="element"/>
		</field>
		<field name="externallyDefined" type="boolean">
			<bind-xml name="externallyDefined" node="element"/>
		</field>
		<field name="enabled" type="boolean">
			<bind-xml name="enabled" node="element"/>
		</field>
		<field name="roleNames" type="com.jaspersoft.jasperserver.export.modules.common.TenantQualifiedName" collection="array">
			<bind-xml name="role" node="element"/>
		</field>
		<field name="attributes" type="com.jaspersoft.jasperserver.export.modules.common.ProfileAttributeBean" collection="array">
			<bind-xml name="attribute" node="element"/>
		</field>
		<field name="previousPasswordChangeTime" type="date">
			<bind-xml name="previousPasswordChangeTime" node="element"/>
		</field>
		<field name="tenantId" type="string">
			<bind-xml name="tenantId" node="element"/>
		</field>
	</class>
</mapping>
