<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>

	<class name="com.jaspersoft.jasperserver.export.modules.scheduling.beans.HolidayCalendarBean">
		<map-to xml="holiday-calendar"/>
        <field name="name" type="string">
            <bind-xml name="name" node="element"/>
        </field>
        <field name="description" type="string">
            <bind-xml name="description" node="element"/>
        </field>
        <field name="timezone" type="string">
            <bind-xml name="timezone" node="element"/>
        </field>
        <field name="excludedDates" type="date" collection="array">
            <bind-xml name="excludedDate" node="element"/>
        </field>
	</class>

</mapping>
