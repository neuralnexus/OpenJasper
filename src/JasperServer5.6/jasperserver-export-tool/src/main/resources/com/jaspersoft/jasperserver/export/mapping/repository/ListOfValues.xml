<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.ListOfValuesBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceBean">
		<map-to xml="listOfValues"/>
		<field name="items" type="com.jaspersoft.jasperserver.export.modules.repository.beans.ListOfValuesItemBean" collection="array">
			<bind-xml name="item" node="element"/>
		</field>
	</class>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.ListOfValuesItemBean">
		<field name="label" type="string">
			<bind-xml name="itemLabel" node="element"/>
		</field>
		<field name="value" type="other">
			<bind-xml name="itemValue" node="element"/>
		</field>
	</class>
</mapping>
