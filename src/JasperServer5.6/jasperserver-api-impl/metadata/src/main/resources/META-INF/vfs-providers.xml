<providers>
    <provider class-name="com.jaspersoft.jasperserver.api.metadata.common.util.RepositoryFileProvider">
        <scheme name="repo"/>
    </provider>
    <provider class-name="com.jaspersoft.jasperserver.api.metadata.common.util.InternalRepositoryFileProvider">
        <scheme name="repoint"/>
    </provider>
</providers>
