  <td align="right" class="xform-close-button" colspan="4">
    <button action="validate" label="fmt:JAJ_000_jsp.ok" handler="com.tonbeller.wcf.form.ButtonHandler" hide="true"/>
    <button action="revert" label="fmt:JAJ_000_jsp.cancel" handler="com.tonbeller.wcf.form.ButtonHandler" hide="true"/>
  </td>
