  <td class="xform-both" colspan="2">
   <div style="margin-left: 3em">fmt:JAJ_000_jsp.jpivot.displayform.numberOfRows<textField type="posint" bookmark="true" size="10" modelReference="extensions(sortRank).topBottomCount"/></div>
   </td>
 </tr>
