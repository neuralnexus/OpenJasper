   <div style="margin-left: 1em">
	<checkBox modelReference="extensions(drillThrough).includeOnlySelectedMeasure"/>fmt:JAJ_000_jsp.jpivot.displayform.includeOnlySelectedMeasure</div>
  </td>
 </tr>

