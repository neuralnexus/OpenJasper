<multiAxisQuery>
    <select>
        <aggregations>
            <aggregation>
                <expression>
                    <string>CountAll(open_date)</string>
                </expression>
                <fieldRef>open_date</fieldRef>
            </aggregation>
            <aggregation>
                <expression>
                    <string>CountDistinct(city)</string>
                </expression>
                <fieldRef>city</fieldRef>
            </aggregation>
            <aggregation>
                <expression>
                    <string>Sum(sales)</string>
                </expression>
                <fieldRef>sales</fieldRef>
            </aggregation>
        </aggregations>
    </select>
    <groupBy>
        <columns>
            <expansions>
                <level>
                    <aggregation>false</aggregation>
                    <expanded>true</expanded>
                    <fieldRef>open_date1</fieldRef>
                </level>
                <member>
                    <expanded>false</expanded>
                    <path>
                        <item>2005</item>
                    </path>
                </member>
                <level>
                    <aggregation>true</aggregation>
                    <expanded>true</expanded>
                </level>
            </expansions>
            <items>
                <allLevel>
                    <field>(All)</field>
                    <dimension>Category</dimension>
                </allLevel>
                <level>
                    <categorizer>year</categorizer>
                    <field>open_date</field>
                    <id>open_date1</id>
                    <includeAll>true</includeAll>
                    <dimension>open_date</dimension>
                </level>
                <level>
                    <categorizer>quarter</categorizer>
                    <field>open_date</field>
                    <id>open_date2</id>
                </level>
                <allLevel>
                    <field>All</field>
                    <dimension>Product</dimension>
                </allLevel>
            </items>
        </columns>
        <rows>
            <expansions>
                <member>
                    <expanded>true</expanded>
                    <path>
                        <item>USA</item>
                        <item>CA</item>
                    </path>
                </member>
            </expansions>
            <items>
                <level>
                    <field>state</field>
                </level>
                <aggregations>
                    <field>__levelAggregations__</field>
                    <id>__levelAggregations__</id>
                </aggregations>
            </items>
        </rows>
    </groupBy>
    <orderBy>
        <member>
            <ascending>false</ascending>
            <path>
                <item>2004</item>
            </path>
        </member>
        <member>
            <ascending>true</ascending>
            <path>
                <item>Q3</item>
            </path>
        </member>
        <level>
            <ascending>false</ascending>
            <fieldRef>open_date1</fieldRef>
        </level>
        <topN>
            <ascending>false</ascending>
            <path>
                <item>store_sales</item>
                <item>USA</item>
            </path>
            <createOtherBucket>false</createOtherBucket>
            <limit>5</limit>
            <limitAllLevels>true</limitAllLevels>
        </topN>
        <bottomN>
            <ascending>true</ascending>
            <path>
                <item>store_sales</item>
                <item>CA</item>
            </path>
            <createOtherBucket>false</createOtherBucket>
            <limit>5</limit>
            <limitAllLevels>true</limitAllLevels>
        </bottomN>
    </orderBy>
</multiAxisQuery>