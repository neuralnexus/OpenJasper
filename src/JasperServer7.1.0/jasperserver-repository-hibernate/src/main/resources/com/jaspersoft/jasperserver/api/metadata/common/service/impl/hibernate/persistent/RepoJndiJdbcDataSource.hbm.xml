<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<joined-subclass 
			name="com.jaspersoft.jasperserver.api.metadata.jasperreports.domain.impl.datasource.RepoJndiJdbcDataSource"
			extends="com.jaspersoft.jasperserver.api.metadata.common.service.impl.hibernate.persistent.RepoResource"
			table="JIJNDIJdbcDatasource">
		<key column="id"/>
		<property name="jndiName" not-null="true" length="100" type="string" column="jndiName"/>
		<property name="timezone" length="100" type="string" column="timezone"/>
	</joined-subclass>
</hibernate-mapping>
