<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">

<mapping>
	<class name="com.jaspersoft.jasperserver.export.modules.repository.beans.DataTypeBean" extends="com.jaspersoft.jasperserver.export.modules.repository.beans.ResourceBean">
		<map-to xml="dataType"/>
		<field name="type" type="byte">
			<bind-xml name="type" node="element"/>
		</field>
		<field name="maxLength" type="integer">
			<bind-xml name="maxLength" node="element"/>
		</field>
		<field name="decimals" type="integer">
			<bind-xml name="decimals" node="element"/>
		</field>
		<field name="regularExpr" type="string">
			<bind-xml name="regularExpr" node="element"/>
		</field>
		<field name="minValue" type="other">
			<bind-xml name="minValue" node="element"/>
		</field>
		<field name="maxValue" type="other">
			<bind-xml name="maxValue" node="element"/>
		</field>
		<field name="strictMin" type="boolean">
			<bind-xml name="strictMin" node="element"/>
		</field>
		<field name="strictMax" type="boolean">
			<bind-xml name="strictMax" node="element"/>
		</field>
	</class>
</mapping>
