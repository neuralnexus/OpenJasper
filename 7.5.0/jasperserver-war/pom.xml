    <description>Jasper Server War Deployment</description>
    <build>
        <directory>${my.build.directory}</directory>
        <finalName>jasperserver</finalName>
        <filters>
            <filter>${platform-path}/js.jdbc.properties</filter>
            <filter>${platform-path}/js.quartz.properties</filter>
        </filters>
        <plugins>
            <!--&lt;!&ndash; mvn remote-resources:bundle assembly:single &ndash;&gt;-->
            <!--&lt;!&ndash; mvn remote-resources:bundle &ndash;&gt;-->
            <plugin>
                <artifactId>maven-remote-resources-plugin</artifactId>
                <version>1.5</version>
                <executions>
                    <execution>
                        <goals>
                            <goal>bundle</goal>
                        </goals>
                    </execution>
                </executions>
                <configuration>
                    <resourcesDirectory>${basedir}/shared-config</resourcesDirectory>
                    <includes>
                        <include>**/applicationContext-remote-services.xml</include>
                        <include>**/applicationContext-rest-services.xml</include>
                        <include>**/applicationContext-security-web.xml</include>
                        <include>**/applicationContext-web-services.xml</include>
                        <include>**/applicationContext-settings.xml</include>
                        <include>**/applicationContext-export-import-web.xml</include>
                    </includes>
                </configuration>
            </plugin>

            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-assembly-plugin</artifactId>
                <version>2.3</version>
                <configuration>
                    <finalName>jasperserver-pro-${project.version}</finalName>
                    <descriptors>
                        <file>shared-it-resources-descriptor.xml</file>
                    </descriptors>
                </configuration>
                <executions>
                    <execution>
                        <id>assembly</id>
                        <phase>package</phase>
                        <goals>
                            <goal>attached</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <!-- use config collector -->
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-antrun-plugin</artifactId>
                <version>1.7</version>
                <executions>
                    <execution>
                        <phase>process-resources</phase>
                        <configuration>
                            <tasks>
                                <!-- define ant-contrib tasks here -->
                                <taskdef resource="net/sf/antcontrib/antcontrib.properties" classpathref="maven.plugin.classpath"/>
                                <!--pass through props to ant file -->
                                <property name="js-path" value="${js-path}"/>
                                <property name="js-pro-path" value="${js-pro-path}"/>
                                <property name="platform-path" value="${platform-path}"/>
                                <property name="target" value="${project.build.directory}/jasperserver/WEB-INF"/>
                                <ant antfile="${js-path}/build-config.xml" inheritall="true" inheritrefs="true">
                                    <property name="module" value="ce.webapp"/>
                                </ant>
                                <echo>COMMENT: About to call: ant build.xml for jasperserver war</echo>
                                <ant antfile="build.xml" inheritall="true" inheritrefs="true">
                                    <target name="all"/>
                                </ant>
                            </tasks>
                        </configuration>
                        <goals>
                            <goal>run</goal>
                        </goals>
                    </execution>
                </executions>
                <dependencies>
                    <dependency>
                        <groupId>ant-contrib</groupId>
                        <artifactId>ant-contrib</artifactId>
                        <version>1.0b3</version>
                        <exclusions>
                            <exclusion>
                                <groupId>ant</groupId>
                                <artifactId>ant</artifactId>
                            </exclusion>
                        </exclusions>
                    </dependency>
                </dependencies>
            </plugin>
<!--TODO: Recheck if this commented out section is needed-->
