    <profiles>
        <!-- enable handling between internal and external builds -->
        <profile>
            <id>jaspersoft-dev-fafj</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>disable.fafj</name>
                    <value>false</value>
                </property>
            </activation>
            <dependencies>
                <dependency>
                    <groupId>com.jaspersoft</groupId>
                    <artifactId>jrs-ui</artifactId>
                    <version>${fafBuildArtifactVersionName}</version>
                    <type>zip</type>
                </dependency>
            </dependencies>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-war-plugin</artifactId>
                        <version>2.2</version>
                        <configuration>
                            <webResources>
                                <resource>
                                    <directory>src/main/webappAdditions</directory>
                                    <filtering>true</filtering>
                                </resource>
                            </webResources>
                            <overlays>
                                <overlay>
                                    <groupId>${jpivot-groupId}</groupId>
                                    <artifactId>ji-jpivot-war</artifactId>
                                    <excludes>
                                        <exclude>**/*.jar</exclude>
                                        <exclude>**/*.jsp</exclude>
                                        <exclude>**/mondrian.properties</exclude>
                                    </excludes>
                                </overlay>
                                <overlay>
                                    <groupId>com.jaspersoft</groupId>
                                    <artifactId>jrs-ui</artifactId>
                                    <type>zip</type>
                                </overlay>
                            </overlays>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>dev</id>
            <activation>
                <activeByDefault>false</activeByDefault>
                <property>
                    <name>disable.fafj</name>
                    <value>true</value>
                </property>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-war-plugin</artifactId>
                        <version>2.2</version>
                        <configuration>
                            <webResources>
                                <resource>
                                    <directory>src/main/webappAdditions</directory>
                                    <filtering>true</filtering>
                                </resource>
                            </webResources>
                            <overlays>
                                <overlay>
                                    <groupId>${jpivot-groupId}</groupId>
                                    <artifactId>ji-jpivot-war</artifactId>
                                    <excludes>
                                        <exclude>**/*.jar</exclude>
                                        <exclude>**/*.jsp</exclude>
                                        <exclude>**/mondrian.properties</exclude>
                                    </excludes>
                                </overlay>
                            </overlays>
                        </configuration>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
