<reportInputControl>
    <dataType>
        <pattern>TEST_PATTERN</pattern>
        <strictMax>false</strictMax>
        <strictMin>false</strictMin>
    </dataType>
    <description>TEST_DESCRIPTION</description>
    <id>TEST_ID</id>
    <label>TEST_LABEL</label>
    <mandatory>true</mandatory>
    <masterDependencies>
        <controlId>TEST_MASTER_DEPENDENCY</controlId>
    </masterDependencies>
    <readOnly>true</readOnly>
    <slaveDependencies>
        <controlId>TEST_SLAVE_DEPENDENCY</controlId>
    </slaveDependencies>
    <state>
        <id>TEST_ID</id>
    </state>
    <type>TEST_TYPE</type>
    <uri>TEST_URI</uri>
    <validationRules>
        <dateTimeFormatValidationRule>
            <errorMessage>TEST_ERROR_MESSAGE</errorMessage>
        </dateTimeFormatValidationRule>
        <mandatoryValidationRule>
            <errorMessage>TEST_ERROR_MESSAGE</errorMessage>
        </mandatoryValidationRule>
        <rangeValidationRule>
            <errorMessage>TEST_ERROR_MESSAGE</errorMessage>
        </rangeValidationRule>
        <regexpValidationRule>
            <errorMessage>TEST_ERROR_MESSAGE</errorMessage>
        </regexpValidationRule>
    </validationRules>
    <visible>true</visible>
</reportInputControl>